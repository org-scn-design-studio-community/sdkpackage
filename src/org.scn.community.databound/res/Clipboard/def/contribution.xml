
<component
	id="Clipboard"
	title="Clipboard"
	tooltip="Clipboard - Simple Clipboard"
	icon="res/Clipboard/Clipboard.png"
	handlerType="div"
	propertySheetPath="aps/PropertyPage.html"
	group="ScnCommunityVisualizations"
	databound="true"
	modes="commons m"
>
	
	<requireJs modes="commons m">res/Clipboard/Clipboard</requireJs>

	<cssInclude>res/Clipboard/Clipboard.css</cssInclude>
	<cssInclude>res/_css/simple_formats.css</cssInclude>
	
	
	<property
		id="DIgnoreResults"
		type="boolean"
		title="Ignore Results"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DMode"
		type="String"
		title="Clipboard Mode"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSeparator"
		type="String"
		title="Separator"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DShowKeys"
		type="boolean"
		title="Show Keys instead of Texts"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSwapAxes"
		type="boolean"
		title="Swap Axes"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValue"
		type="String"
		title="Value"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="data"
		type="ResultSet"
		title="Data"
		group="DataBinding"
		visible="false">

		<option name="maxCells" value="100000" />
		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="presentationDelimiter" value=" | " />
	</property>
	<property
		id="picture"
		type="Url"
		title="Url For Picture"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="trigger"
		type="String"
		title="Just As Simple Triger"
		group="Display"
		bindable="true"
		visible="false">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">64</defaultValue>
		<defaultValue property="HEIGHT">32</defaultValue>
		
		<defaultValue property="DIgnoreResults"><![CDATA[true]]></defaultValue>
		<defaultValue property="DMode"><![CDATA[Custom]]></defaultValue>
		<defaultValue property="DSeparator"><![CDATA[;]]></defaultValue>
		<defaultValue property="DShowKeys"><![CDATA[false]]></defaultValue>
		<defaultValue property="DSwapAxes"><![CDATA[false]]></defaultValue>
		<defaultValue property="DValue"><![CDATA[]]></defaultValue>
		<defaultValue property="data"><![CDATA[null]]></defaultValue>
		<defaultValue property="picture"><![CDATA[]]></defaultValue>
		<defaultValue property="trigger"><![CDATA[]]></defaultValue>

	</initialization>

</component>
