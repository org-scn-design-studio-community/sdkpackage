<core:View
  xmlns="sap.package"
  xmlns:core="sap.ui.core"
  xmlns:l="sap.ui.layout"
  xmlns:m="sap.m"
  
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
  
  controllerName="org.scn.community.%COMP-package%.%COMP-id%.Controller"
  >
  
  <l:VerticalLayout
    width="100%" content="{/}">
    <l:content
		width="{width}"
	>
		<PlanningCalendar
				appointmentSelectedKey="{appointmentSelectedKey}"
				contentHeight="{contentHeight}"
				contentWidth="{contentWidth}"
				intervalSelectedKey="{intervalSelectedKey}"
				noDataText="{noDataText}"
				onAppointmentSelected="{onAppointmentSelected}"
				onIntervalSelected="{onIntervalSelected}"
				onRowSelectionChangeed="{onRowSelectionChangeed}"
				onStartDateChangeed="{onStartDateChangeed}"
				onViewChangeed="{onViewChangeed}"
				rowSelectionChangeedKey="{rowSelectionChangeedKey}"
				rows="{rows}"
				showIntervalHeaders="{showIntervalHeaders}"
				showRowHeaders="{showRowHeaders}"
				singleSelection="{singleSelection}"
				startDate="{startDate}"
				startDateChangeedKey="{startDateChangeedKey}"
				viewChangeedKey="{viewChangeedKey}"
				viewKey="{viewKey}"
		
		>
	     	
			
		
		
		
		
		<onAppointmentSelected>
			
		</onAppointmentSelected>
		<onIntervalSelected>
			
		</onIntervalSelected>
		<onRowSelectionChangeed>
			
		</onRowSelectionChangeed>
		<onStartDateChangeed>
			
		</onStartDateChangeed>
		<onViewChangeed>
			
		</onViewChangeed>
		
		
		
		
		
		
		
		
		
		

		</PlanningCalendar>
	</l:content>
  </l:VerticalLayout> 
</core:View>